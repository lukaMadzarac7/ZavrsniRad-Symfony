{% extends 'base.html.twig' %}

{% block title %}ServiceImage index{% endblock %}

{% block body %}
    <h1>ServiceImage index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Service_id</th>
                <th>Image</th>
                <th>Created_at</th>
                <th>Updated_at</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for service_image in service_images %}
            <tr>
                <td>{{ service_image.id }}</td>
                <td>{{ service_image.serviceId }}</td>
                <td>{{ service_image.image }}</td>
                <td>{{ service_image.createdAt ? service_image.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ service_image.updatedAt ? service_image.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_service_image_show', {'id': service_image.id}) }}">show</a>
                    <a href="{{ path('app_service_image_edit', {'id': service_image.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_service_image_new') }}">Create new</a>
{% endblock %}
