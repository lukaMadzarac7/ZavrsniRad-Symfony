{% block title %}ServiceImage{% endblock %}

{% block body %}
    <h1>ServiceImage</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ service_image.id }}</td>
            </tr>
            <tr>
                <th>Service</th>
                <td>{{ service_image.service }}</td>
            </tr>
