{% extends 'admin/base.html.twig' %}

{% block title %}UserRating{% endblock %}

{% block body %}
    <h1>UserRating</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user_rating.id }}</td>
            </tr>
            <tr>
                <th>User</th>
                <td>{{ user_rating.user }}</td>
            </tr>
            <tr>
                <th>Rating</th>
                <td>{{ user_rating.rating }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ user_rating.createdAt ? user_rating.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ user_rating.updatedAt ? user_rating.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_user_rating_index') }}">back to list</a>

    <a href="{{ path('app_user_rating_edit', {'id': user_rating.id}) }}">edit</a>

    {{ include('admin/user_rating/_delete_form.html.twig') }}
{% endblock %}
